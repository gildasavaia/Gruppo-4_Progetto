from itertools import product
i = 4
#dataset
data = ["version_1.csv", "version_2.xlsx", "version_3.txt", "version_4.json",
"version_5.tsv"]
#tecniche di validazione
tech = ["Random Subsampling", "K-fold Cross Validation", 
        "Leave-one-out Cross Validation", 
"Leave-p-out Cross Validation", "Stratified Cross Validation",
"Stratified Shuffle Split", "Bootstrap"]
A, B, C = list(product(data, tech, tech))[i]
print("A:", A, "B:", B, "C:", C)

#dobbiamo usare il dataset A con queste due tecniche, ovvero
#Random subsampling e Stratified cross validation
